
@{
    ViewBag.Title = "AptitudeTestList";
}



<html ng-app="myApp">
<head>
    <title></title>
    <script src="~/Content/angularjs-dropdown-multiselect.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/lodash.js/2.4.1/lodash.min.js"></script>
    <script src="~/Content/Controller/AptitudeTestController.js"></script>
    @*<script src="~/Content/Module.js"></script>

        <script src="~/Content/dirPagination.js"></script>*@

    <style>
        table {
            border-collapse: collapse;
            border-spacing: 0;
            width: 100%;
            border: 1px solid #ddd;
        }

        th, td {
            border: none;
            text-align: left;
            padding: 8px;
        }
    </style>

</head>

<body>
    <div class="container" style="background-color:white;" ng-controller="coodinatorcontroller">
        <br />

        <h2><center>Aptitude_Test List</center></h2>

        <form class="form-inline">
            <div class="form-group" style="float:left;">
            </div>

            <div style="float:right;padding-right:75px;">
            </div>
            <div style="clear:both;"></div>
        </form>
        <br />

        <table cellpadding="12" class="table table-bordered table-hover table-striped">
            <tr class="success">
                <th style="cursor: pointer;" ng-click="sort('Date')"><b>Date</b></th>
                <th style="cursor: pointer;" ng-click="sort('Branch_id')"><b>BranchName</b></th>

            </tr>
            <tr dir-paginate="employee in CodData | orderBy:sortKey:reverse|filter:search:strict|itemsPerPage:7"
                ng-model="search">
                <td>
                    {{employee.Date}}
                </td>
                <td>
                    {{employee.BranchName}}
                </td>

            </tr>
            <b style="color: #5bc0de;">Total Records </b>:

            <b>{{ (CodData | filter: search:strict).length }}of</b>


            @*<b>{{filtered.length}}  all </b>*@
            <b>{{CodData.length}}</b>
        </table>

        <dir-pagination-controls max-size="10"
                                 direction-links="true"
                                 boundary-links="true">
        </dir-pagination-controls>


    </div>
</body>
</html>